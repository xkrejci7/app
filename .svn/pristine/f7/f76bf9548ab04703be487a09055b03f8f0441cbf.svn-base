    <body> 
        <wicket:extend>
            <h1>XPath</h1>
            
            <span wicket:id="task">[[ ]]</span>
            
            <form wicket:id="form_xpath">
                <input type="text" name="expression" value="" wicket:id="expression" />
                <input type="submit" value="Vyhodnotit" />
                
                <div wicket:id="result"></div>
                
            </form>            
            
            <p>&nbsp;</p>

        </wicket:extend>
