package cz.muni.fi.wicket;

import cz.muni.fi.exercise.DTD;
import cz.muni.fi.exercise.XPath;
import org.apache.wicket.markup.html.link.BookmarkablePageLink;
import org.apache.wicket.markup.html.panel.Panel;

/**
 *
 * @author Franta
 */
public class MenuPanel extends Panel {

    public MenuPanel(String componentName) {
        super(componentName);
        
        add(new BookmarkablePageLink("home", HomePage.class));
        add(new BookmarkablePageLink("xpath", XPath.class));
        add(new BookmarkablePageLink("dtd", DTD.class));
        
        
    }    
}
